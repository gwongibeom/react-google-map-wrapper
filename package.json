{
  "name": "react-google-map-wrapper",
  "version": "0.0.1-beta.7",
  "description": "Component library for rendering Google Maps in React",
  "keywords": [
    "react",
    "reactjs",
    "google",
    "map",
    "maps",
    "google map",
    "reaect google map",
    "google map component"
  ],
  "homepage": "https://github.com/pyjun01/react-google-map-wrapper#readme",
  "bugs": {
    "url": "https://github.com/pyjun01/react-google-map-wrapper/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pyjun01/react-google-map-wrapper.git"
  },
  "funding": "https://github.com/sponsors/pyjun01",
  "license": "MIT",
  "author": "Yongjun Park",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "bump": "pnpm run build && pnpx bumpp && npm publish",
    "example": "cd example && pnpm i && pnpm dev",
    "lint:fix": "eslint --fix",
    "test": "vitest --globals"
  },
  "peerDependencies": {
    "@types/google.maps": ">=3.50.5"
  },
  "dependencies": {
    "@googlemaps/markerclusterer": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.5"
  },
  "devDependencies": {
    "@mvlchain/eslint-config": "^1.0.3",
    "@mvlchain/prettier-config": "^1.0.1",
    "@types/google.maps": "^3.54.10",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "bumpp": "^9.2.0",
    "eslint": "^8.54.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.3.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  }
}
