{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "lib": ["dom", "es2021"],
    "outDir": "dist",
    "moduleResolution": "node",
    "module": "es2022",
    "target": "es2021",
    "noImplicitAny": false,
    "types": ["vite/client", "google.maps", "node"],
    "paths": {
      "@@*": ["./src/*"]
    }
  },
  "include": ["./**/*.ts", "./**/*.tsx"],
  "exclude": ["node_modules", "dist", "*.d.ts"]
}
